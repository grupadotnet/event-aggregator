node {
    deleteDir()

    try {
        stage ('Clone') {
        	checkout scm
        }
        stage ('Build') {
            bat "\"${tool 'MSBuild'}\" C:\\Program Files (x86)\\Jenkins\\workspace\EventAggregator_Jenkins\\Event-Aggregator.sln /p:Configuration=Release /p:Platform=\"Any CPU\" /p:ProductVersion=1.0.0.${env.BUILD_NUMBER}"
        }
    } catch (err) {
        currentBuild.result = 'FAILED'
        throw err
    }
}