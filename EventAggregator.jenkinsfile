node {
    deleteDir()

    try {
        stage ('Clone') {
        	checkout scm
        }
        stage ('Build') {
            bat "msbuild.exe Event-Aggregator.sln /p:Configuration=Release /property:OutDir=bin/publish /p:ProductVersion=1.0.0.${env.BUILD_NUMBER}"
        }
    } catch (err) {
        currentBuild.result = 'FAILED'
        throw err
    }
}