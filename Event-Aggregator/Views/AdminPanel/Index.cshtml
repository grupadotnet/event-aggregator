@{
    ViewData["Title"] = "Panel administratora";
}
@model Event_Aggregator.Models.User

<link rel="stylesheet" href="~/css/LoginPanel.css" asp-append-version="true" />
</br>
</br>
<form asp-controller="AdminPanel" asp-action="Panel" method="post">
    <div class="container adminpanel">
        <input type="text" asp-for="Login" placeholder="Enter Username" required>
        <input type="password" asp-for="Password" placeholder="Enter Password" required>

        <button type="submit">Login</button>
    </div>
</form>
